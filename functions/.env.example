# Firebase Functions Environment Variables
# Copy this file to .env and fill in your actual values

# Google AI (Gemini) API Key
# Get from: https://makersuite.google.com/app/apikey
GOOGLE_AI_API_KEY=your-google-ai-api-key-here

# ElevenLabs API Key
# Get from: https://elevenlabs.io/app/settings/api-keys
ELEVENLABS_API_KEY=your-elevenlabs-api-key-here

# Gemini Callback Secret (for webhook authentication)
# Generate a random secure string
GEMINI_CALLBACK_SECRET=your-callback-secret-here

# ElevenLabs Voice IDs (optional - configured in Firestore AI settings)
# These are fallback values if not configured in database
# Get from: https://elevenlabs.io/app/voice-library
ELEVENLABS_HOST1_VOICE=
ELEVENLABS_HOST2_VOICE=

# Note: Voice IDs are primarily managed in Firestore at settings/ai
# You can set them here as fallback, but database settings take precedence

# Sentry Error Tracking
# Get DSN from: https://sentry.io → Project Settings → Client Keys (DSN)
SENTRY_DSN=
SENTRY_ENVIRONMENT=production
